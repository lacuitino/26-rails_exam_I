<div class="row">
  <div class="col-md-6">
    <% @todos.each do |x| %>
    <% if x.completed == false %>
    <%= x.description  %>
    <% end %>
    <br>
    <% end %>
  </div>

  <div class="col-md-6">
    <% @todos.each do |x| %>
    <% if x.completed == true %>
    <%= x.description  %>
    <% end %>
    <br>
    <% end %>
  </div>

</div>
