<%= link_to "Crear", todos_new_path, class: 'btn btn-info' %>

<% @todos.each do |x| %>
<%= x.description %>
<%= x.completed%>

<%= link_to "Mostrar",x, class: 'btn btn-info' %>
<%= link_to "Editar", edit_todo_path(x), class: 'btn btn-info' %>
<%= link_to "Destroy", x, method: :delete, class: 'btn btn-info' %>
<%= link_to "Completed", complete_todo_path(x), class: 'btn btn-info' %>
<% end %>
